<?xml version="1.0" encoding="utf-8"?>
<launch>
    <!--
    Kuka KR16 Interface
    Author : Theppasith N. <theppasith.n@hiveground.com>
    Dependencies :
        - kuka_experimental ROS Repo
        - kuka_rsi_hw_interface (from kuka_experimental)
        - kuka_kr16_support (from kuka_experimental) -->
    <!-- RUN RSI SERVER / SIM -->
    <include file="$(find kuka_moveit_config)/launch/bringup/rsi_server.launch" />
    <!-- LOAD ROBOT CONTROLLER -->
    <include file="$(find kuka_moveit_config)/launch/bringup/hardware_controller.launch">
        <param name="sim" value="false"/>
    </include>

    <!-- MoveIt! KR16 -->

</launch>